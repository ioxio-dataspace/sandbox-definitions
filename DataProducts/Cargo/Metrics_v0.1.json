        "type": "object",
        "title": "HTTPValidationError"
      },
      "NotFound": {
        "properties": {
          "type": {
            "type": "string",
            "pattern": "^[a-z0-9_]*$",
            "title": "Error type",
            "description": "An identifier for the type of error.",
