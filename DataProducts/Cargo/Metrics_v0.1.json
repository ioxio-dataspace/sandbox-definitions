            "type": "string",
            "pattern": "^[a-z0-9_]*$",
            "title": "Error type",
            "description": "An identifier for the type of error.",
            "examples": ["api_token_missing_or_invalid"]
          },
          "message": {
            "type": "string",
            "title": "Error message",
            "description": "A human readable description of the error.",
            "examples": ["The API token has expired"]
