      "post": {
        "summary": "draft/Key/DeleteAssignment",
        "description": "Delete assignment",
        "operationId": "request_draft_Key_DeleteAssignment",
        "parameters": [
          {
            "description": "Optional consent token",
            "required": false,
            "schema": {
              "title": "X-Consent-Token",
